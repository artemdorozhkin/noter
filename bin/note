#!/usr/bin/env ruby

ENV["BUNDLE_GEMFILE"] ||= File.expand_path("../../Gemfile", __FILE__)
begin
  require "bundler/setup"
  require "colorize"
rescue LoadError
  warn "Bundler is not installed. Please run `gem install bundler && bundle install`"
  exit 1
end
require_relative "../noter/main.rb"

Noter.main